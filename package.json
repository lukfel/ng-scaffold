{
  "name": "ng-scaffold-app",
  "version": "21.1.30",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --browsers ChromeHeadless --watch false",
    "lint": "ng lint --fix",
    "deploy": "ng deploy --project ng-scaffold-app --base-href /ng-scaffold/ --repo https://github.com/lukfel/ng-scaffold.git",
    "lib:build": "ng build ng-scaffold --configuration production",
    "lib:watch": "ng build ng-scaffold --watch --configuration development",
    "lib:schematics": "cd projects/ng-scaffold && npm run build && npm run postbuild",
    "lib:publish": "npm publish ./dist/ng-scaffold --access public"
  },
  "dependencies": {
    "@angular/animations": "^21.0.8",
    "@angular/cdk": "^21.0.6",
    "@angular/common": "^21.0.8",
    "@angular/compiler": "^21.0.8",
    "@angular/core": "^21.0.8",
    "@angular/forms": "^21.0.8",
    "@angular/material": "^21.0.6",
    "@angular/platform-browser": "^21.0.8",
    "@angular/router": "^21.0.8",
    "@angular/service-worker": "^21.0.8",
    "@lukfel/ng-scaffold": "file:dist/ng-scaffold",
    "marked": "^17.0.0",
    "ngx-markdown": "^21.0.0",
    "rxjs": "~7.8.0"
  },
  "devDependencies": {
    "@angular-eslint/builder": "21.1.0",
    "@angular-eslint/eslint-plugin": "21.1.0",
    "@angular-eslint/eslint-plugin-template": "21.1.0",
    "@angular-eslint/schematics": "21.1.0",
    "@angular-eslint/template-parser": "21.1.0",
    "@angular/build": "^21.0.5",
    "@angular/cli": "^21.0.5",
    "@angular/compiler-cli": "^21.0.8",
    "@types/jasmine": "~4.3.0",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "angular-cli-ghpages": "^2.0.3",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "21.0.0",
    "prettier": "^3.4.1",
    "typescript": "~5.9.3",
    "copyfiles": "~2.4.1"
  }
}
